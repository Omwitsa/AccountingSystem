  <DataSets>
    <DataSet Name="ProfitLossSet">
      <Query>
        <DataSourceName>DS_Dango</DataSourceName>
        <CommandText>SELECT a.Code, a.Name, a.Type, cj.Label, (cj.Credit - cj.Debit) AS Amount FROM CInvoiceJournal cj INNER JOIN AccountCharts a ON cj.GlAccount = a.Code WHERE a.Type = 'Income'
UNION
SELECT a.Code, a.Name, a.Type, cj.Label, (cj.Debit - cj.Credit) AS Amount FROM CreditNoteJournals cj INNER JOIN AccountCharts a ON cj.GlAccount = a.Code WHERE a.Type = 'Income'
UNION
SELECT a.Code, a.Name, a.Type, cj.Label, (cj.Debit - cj.Credit) AS Amount FROM BillJournals cj INNER JOIN AccountCharts a ON cj.GlAccount = a.Code WHERE a.Type = 'Expense'
UNION
SELECT a.Code, a.Name, a.Type, cj.Label, (cj.Credit - cj.Debit) AS Amount FROM RefundJournals cj INNER JOIN AccountCharts a ON cj.GlAccount = a.Code WHERE a.Type = 'Expense'</CommandText>
      </Query>
